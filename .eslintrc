{
  "extends": ["airbnb-base", "airbnb-typescript/base"],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "ignorePatterns": ["node_modules", "dist", ".yarn"],
  "rules": {
    "no-nested-ternary": 0,
    "max-len": 0,
    "no-console": 0,
    "max-classes-per-file": 0,
    "no-return-assign": 0,
    "no-underscore-dangle": 0,
    "import/export": 0
  }
}
